# EGroupware Configuration
EGROUPWARE_BASE_URL=https://your-domain.com/egroupware
EGROUPWARE_USERNAME=your_username
EGROUPWARE_PASSWORD=your_password

# Embedding Service
EMBEDDING_PROVIDER=ionos
EMBEDDING_API_KEY=your_api_key
EMBEDDING_MODEL=BAAI/bge-m3

# LLM Service
LLM_PROVIDER=ionos
LLM_API_KEY=your_api_key
LLM_MODEL=meta-llama/Llama-3.3-70B-Instruct

# Qdrant Configuration
QDRANT_MODE=disk
QDRANT_PATH=./qdrant_storage

# Server Configuration
SERVER_HOST=0.0.0.0
SERVER_PORT=5002
SERVER_DEBUG=false
FLASK_SECRET_KEY=change-this-to-a-random-secret-key

